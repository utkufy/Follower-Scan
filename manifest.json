{
    "manifest_version": 2,
    "name": "My Extension",
    "description": "This is my sample extension",
    "version": "1.0",
    "background": {
        "scripts": ["./background.js"]
    },
   
    "browser_action": {
        "default_popup": "popup.html"
       
    },
    "permissions": [
        "tabs",
        "https://www.instagram.com/*"

]
}